[Unit]
Description=Service control PWM fans via RPi GPIO
After=network.target

[Service]
User=pi
Group=pi
WorkingDirectory=/home/pi/pi_fan_app
Environment="PATH=/home/pi/pi_fan_app/pi_fan_env/bin"
ExecStart=/home/pi/pi_fan_app/pi_fan_env/python /home/pi/pi_fan_app/fans_controls.py

[Install]
WantedBy=multi-user.target